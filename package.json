    "@types/react-dom": "npm:types-react-dom@rc",
    "class-variance-authority": "latest",
    "clsx": "latest",
    "colorize-js": "link:/Users/coderchris/Desktop/libraries/colorize-js",
    "concurrently": "latest",
    "eslint": "8.57.0",
    "eslint-config-next": "canary",
    "postcss": "latest",
